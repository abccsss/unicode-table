{
    "compilerOptions": {
        "outDir": "./out/",
        "noImplicitAny": false,
        "removeComments": true,
        "moduleResolution": "node",
        "module": "commonjs",
        "target": "ES5",
        "sourceMap": true
    },
    "include": [
        "src/**/*"
    ],
    "exclude": [
        "node_modules/**/*"
    ]
}